<?xml version="1.0" encoding="UTF-8" ?>
<litepal>
    <!--数据库的名字-->
    <dbname value="Yaoqi"></dbname>
    <version value="1"></version>
    <list>
        <!--登录用户的信息-->
        <mapping class="com.example.zexiger.yaoqi.database.InitClass"/>
        <!--漫画的对应下载-->
        <mapping class="com.example.zexiger.yaoqi.database.LoadClass"/>
        <!--记住订阅收藏里面的信息-->
        <mapping class="com.example.zexiger.yaoqi.database.UpdateClass"/>
        <!--缓存-->
        <mapping class="com.example.zexiger.yaoqi.database.LoadClass_0"/>
    </list>
</litepal>